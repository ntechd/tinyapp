<header>
  <nav class="navbar navbar-expand-lg navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav mr-auto">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
      <% if (user) { %>
        <form class="form-inline my-2 my-lg-0" method="POST" action="/logout">
          
          <span class="m-2 my-lg-0">Logged In As: <%= user['email'] %></span>
          <button class="btn btn-outline-dark my-2 my-sm-0" type="submit">Logout</button>
        </form>
      <% } else { %> 

        <a href="/register" class="btn btn-outline-dark my-2 mx-2 my-sm-0">Register</a>
        <a href="/login" class="btn btn-outline-dark my-2 my-sm-0">Login</a>
        
      <% } %>
    </div>
  </nav>
</header>